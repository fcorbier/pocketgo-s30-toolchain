#!/usr/bin/env bash

WRAPPER_INCLUDEDIR="${WRAPPER_INCLUDEDIR:=-I${STAGING_DIR:-/opt/disk/part2/guoxs/zkswe_tina3.5/tina/out/astar-zkswe/staging_dir/target}/usr/include/uClibc++/}"
WRAPPER_LIBDIR="${WRAPPER_LIBDIR:=-L${STAGING_DIR:-/opt/disk/part2/guoxs/zkswe_tina3.5/tina/out/astar-zkswe/staging_dir/target}/lib/}"

WRAPPER_OPTIONS=""
WRAPPER_LIBS="-lc -L/opt/disk/part2/guoxs/zkswe_tina3.5/tina/prebuilt/gcc/linux-x86/arm/toolchain-sunxi-musl/toolchain/lib/ -lgcc_s -lssp_nonshared "

WRAPPER_INCLIB="Y"
while [ -n "$1" ]
do
	WRAPPER_OPTIONS="$WRAPPER_OPTIONS $1"
	if [ "$1" = "-c" -o "$1" = "-E" -o "$1" = "-S" -o "$1" = "-MF" ]
	then
		WRAPPER_INCLIB="N"
	fi
	if [ "$1" = "-static" ]
	then
		WRAPPER_LIBS="-lc -L/opt/disk/part2/guoxs/zkswe_tina3.5/tina/prebuilt/gcc/linux-x86/arm/toolchain-sunxi-musl/toolchain/lib/ -lgcc_s -lssp_nonshared -lgcc_eh"
	fi
	shift
done
if [ "$WRAPPER_INCLIB" = "Y" ]
then
	WRAPPER_OPTIONS="$WRAPPER_OPTIONS -nodefaultlibs $WRAPPER_LIBDIR -luClibc++ $WRAPPER_LIBS"
fi

exec arm-openwrt-linux-muslgnueabi-g++ -std=gnu++98 -nostdinc++ -nostdinc++ -DGCC_HASCLASSVISIBILITY  $WRAPPER_INCLUDEDIR $WRAPPER_OPTIONS
